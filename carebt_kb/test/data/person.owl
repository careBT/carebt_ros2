<swrl:Imp>
  <swrl:body>
    <rdf:Description>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#Person"/>
          <swrl:argument1 rdf:resource="urn:swrl#person"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest>
        <rdf:Description>
          <rdf:first>
            <swrl:DatavaluedPropertyAtom>
              <swrl:propertyPredicate rdf:resource="#weight"/>
              <swrl:argument1 rdf:resource="urn:swrl#person"/>
              <swrl:argument2 rdf:resource="urn:swrl#weight"/>
            </swrl:DatavaluedPropertyAtom>
          </rdf:first>
          <rdf:rest>
            <rdf:Description>
              <rdf:first>
                <swrl:DatavaluedPropertyAtom>
                  <swrl:propertyPredicate rdf:resource="#size"/>
                  <swrl:argument1 rdf:resource="urn:swrl#person"/>
                  <swrl:argument2 rdf:resource="urn:swrl#size"/>
                </swrl:DatavaluedPropertyAtom>
              </rdf:first>
              <rdf:rest>
                <rdf:Description>
                  <rdf:first>
                    <swrl:BuiltinAtom>
                      <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#multiply"/>
                      <swrl:arguments>
                        <rdf:Description>
                          <rdf:first rdf:resource="urn:swrl#size2"/>
                          <rdf:rest>
                            <rdf:Description>
                              <rdf:first rdf:resource="urn:swrl#size"/>
                              <rdf:rest>
                                <rdf:Description>
                                  <rdf:first rdf:resource="urn:swrl#size"/>
                                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                </rdf:Description>
                              </rdf:rest>
                            </rdf:Description>
                          </rdf:rest>
                        </rdf:Description>
                      </swrl:arguments>
                    </swrl:BuiltinAtom>
                  </rdf:first>
                  <rdf:rest>
                    <rdf:Description>
                      <rdf:first>
                        <swrl:BuiltinAtom>
                          <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#divide"/>
                          <swrl:arguments>
                            <rdf:Description>
                              <rdf:first rdf:resource="urn:swrl#bmi"/>
                              <rdf:rest>
                                <rdf:Description>
                                  <rdf:first rdf:resource="urn:swrl#weight"/>
                                  <rdf:rest>
                                    <rdf:Description>
                                      <rdf:first rdf:resource="urn:swrl#size2"/>
                                      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                  </rdf:rest>
                                </rdf:Description>
                              </rdf:rest>
                            </rdf:Description>
                          </swrl:arguments>
                        </swrl:BuiltinAtom>
                      </rdf:first>
                      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                    </rdf:Description>
                  </rdf:rest>
                </rdf:Description>
              </rdf:rest>
            </rdf:Description>
          </rdf:rest>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </swrl:body>
  <swrl:head>
    <rdf:Description>
      <rdf:first>
        <swrl:DatavaluedPropertyAtom>
          <swrl:propertyPredicate rdf:resource="#bmi"/>
          <swrl:argument1 rdf:resource="urn:swrl#person"/>
          <swrl:argument2 rdf:resource="urn:swrl#bmi"/>
        </swrl:DatavaluedPropertyAtom>
      </rdf:first>
      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    </rdf:Description>
  </swrl:head>
</swrl:Imp>


